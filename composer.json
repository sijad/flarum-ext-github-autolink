{
    "name": "sijad/flarum-ext-github-autolink",
    "description": "Autolink github Issues and commits",
    "type": "flarum-extension",
    "keywords": ["github", "autolink", "issue", "commit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sajjad Hashemian",
            "email": "wolaws@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sijad/flarum-ext-github-autolink/issues",
        "source": "https://github.com/sijad/flarum-ext-github-autolink"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.4"
    },
    "autoload": {
        "psr-4": {
            "Sijad\\GithubAutolink\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Github Autolink",
            "icon": {
                "name": "github-alt",
                "backgroundColor": "#eaeaea",
                "color": "#4078c0"
            }
        }
    }
}
